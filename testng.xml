<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Automation Practice Test Suite" parallel="methods" thread-count="2" >
  <test name="UI Automation Test" >
      <parameter name="browser" value="${browser}"></parameter>
      <parameter name="isLambdaTest" value="${isLambdaTest}"></parameter>
      <parameter name="isHeadless" value="${isHeadless}"></parameter>
      <classes>
          <class name="com.ui.tests.LoginTest"></class>
          <class name="com.ui.tests.InvalidCredsLoginTest"></class>
          <class name="com.ui.tests.AddNewFirstAddressTest"></class>
          <class name="com.ui.tests.SearchProductTest"></class>
          <class name="com.ui.tests.ProductCheckoutTest"></class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
